{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "SchoolManagmentRelase": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "app",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "progress": true,
                        "extractCss": true,
                        "outputPath": "dist/dashboard",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "assets": [
                            "src/assets",
                            "src/manifest.json",
                            {
                                "glob": "mode-json.js",
                                "input": "./node_modules/ace-builds/src-min/",
                                "output": "/"
                            }
                        ],
                        "styles": [
                            "src/assets/css/bootstrap.min.css",
                            "src/assets/css/style.css",
                            "src/assets/font.css",
                            "src/assets/css/plugins/jsTree/style.min.css",
                            "src/assets/scrollem/trackpad-scroll-emulator.css",
                            "src/assets/css/plugins/bootstrap-rtl/bootstrap-rtl.min.css",
                            "src/assets/font-awesome/css/font-awesome.css",
                            "src/assets/css/animate.css",
                            "node_modules/metismenu/dist/metisMenu.min.css",
                            "src/assets/fonts/material/material-icons.css",
                            "src/styles.scss"
                        ],
                        "scripts": [
                            "src/assets/js/jquery-3.1.1.min.js",
                            "src/assets/js/bootstrap.min.js",
                            "src/assets/js/plugins/slimscroll/jquery.slimscroll.min.js",
                            "node_modules/jstree/dist/jstree.min.js",
                            "node_modules/hammerjs/hammer.min.js",
                            "src/assets/js/plugins/wow/wow.min.js",
                            "src/assets/js/plugins/metisMenu/jquery.metisMenu.js",
                            "src/assets/toastr/toastr.min.js",
                            "src/assets/scrollem/jquery.trackpad-scroll-emulator.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "serviceWorker": true,
                            "ngswConfigPath": "src/ngsw-config.json"
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "SchoolManagmentRelase:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "SchoolManagmentRelase:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "SchoolManagmentRelase:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.spec.json",
                        "karmaConfig": "src/karma.conf.js",
                        "styles": [
                            "styles.css"
                        ],
                        "scripts": [],
                        "assets": [
                            "src/assets",
                            "src/manifest.json"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/tsconfig.app.json",
                            "src/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "server": {
                    "builder": "@angular-devkit/build-angular:server",
                    "options": {
                        "outputPath": "dist-server",
                        "main": "src/main.ts",
                        "tsConfig": "src/tsconfig.server.json"
                    },
                    "configurations": {
                        "dev": {
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": true
                        },
                        "production": {
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false
                        }
                    }
                }
            }
        },
